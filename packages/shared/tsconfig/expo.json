{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Native",
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-native",
    "lib": ["DOM", "ESNext"],
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "moduleSuffixes": [".native", ".web", ""]
  }
}
